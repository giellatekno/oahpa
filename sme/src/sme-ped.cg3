REMOVE:r1003 N IF (-1 (N Gen) OR (Pron Pers Gen))(NEGATE -1 Nom)(0 Po)(NEGATE 0 NOUN-PO); 
	## Daid ovddas gal in máksán maidege.

SELECT:r1004 N IF (*-1 (Dem Acc) BARRIER NPNH LINK *-1C VFIN BARRIER NOT-ADV-PCLE)(0 Acc)(0 VFIN)(NEGATE 0 PrfPrc LINK *-1 ("leat"));
	## Áhčči dagai dan fatnasa nu oanehačča.

REMOVE:r1005 PrsPrc IF (-1 A)(NEGATE -1 Adv)(NEGATE 1 Ess); 
	## Dainnna ii leat vissis mearri galljodagas.

SELECT:r1006 A IF (-1 ("eambbo"))(NEGATE 0 Comp);
